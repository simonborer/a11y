---
isHome: true
---
<div class="grid-container">
  <div class="grid-x">
    <div class="cell small-10 small-offset-1">
      <table class="stack unstriped weeks-table">
        <% site.weeks.forEach((week, index) => { %>
          <tr>
            <th rowspan="2">Week <%- index %></th>
            <td colspan="2"><%- week.description %></td>
          </tr>
          <tr>
            <td>
              <div class="button-group">
                <a class="button" href="./notes/week-<%- index %>">Notes <span class="show-for-sr">for week <%- index %></span></a>
                <a class="button" href="./slides/notes/week-<%- index %>">Slides <span class="show-for-sr">for week <%- index %></span></a>
              </div>
            </td>
          </tr>
        <% }) %>
      </table>
    </div>
  </div>
</div>

